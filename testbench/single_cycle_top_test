    %store/vec4 v0x623e9a77a800_0, 0, 5;
    %load/vec4 v0x623e9a77a370_0;
    %parti/s 3, 12, 5;
    %store/vec4 v0x623e9a75a290_0, 0, 3;
    %vpi_call 2 54 "$display", "PC: 0x%08x | Instruction: 0x%08x", v0x623e9a77a510_0, v0x623e9a77a370_0 {0 0 0};
