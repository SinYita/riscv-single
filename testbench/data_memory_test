    %store/vec4 v0x5cd582bd2320_0, 0, 32;
    %wait E_0x5cd582b88d80;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cd582bd23c0_0, 0, 1;
    %delay 5000, 0;
    %vpi_call 2 262 "$display", "%0t\011%b\011%b\011%08x\011%08x\011%08x\011Clock Edge", $time, v0x5cd582bd2640_0, v0x5cd582bd23c0_0, v0x5cd582bd21a0_0, v0x5cd582bd2320_0, v0x5cd582bd2280_0 {0 0 0};
    %load/vec4 v0x5cd582bd2280_0;
    %cmpi/e 3435973836, 0, 32;
    %jmp/0xz  T_3.19, 4;
    %vpi_call 2 266 "$display", "\342\234\223 PASS: Write occurs on positive clock edge" {0 0 0};
    %jmp T_3.20;
T_3.19 ;
    %vpi_call 2 268 "$display", "\342\234\227 FAIL: Clock edge timing issue. Expected CCCCCCCC, got %08x", v0x5cd582bd2280_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cd582bd27d0_0, 0, 1;
T_3.20 ;
    %load/vec4 v0x5cd582bd2710_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5cd582bd2710_0, 0, 32;
    %vpi_call 2 274 "$display", "\012=== Data_Memory Testbench Complete ===" {0 0 0};
    %load/vec4 v0x5cd582bd27d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.21, 8;
    %vpi_call 2 276 "$display", "\360\237\216\211 ALL TESTS PASSED! (%0d/%0d)", v0x5cd582bd2710_0, v0x5cd582bd2710_0 {0 0 0};
    %jmp T_3.22;
T_3.21 ;
    %vpi_call 2 278 "$display", "\342\235\214 SOME TESTS FAILED!" {0 0 0};
T_3.22 ;
    %delay 50000, 0;
    %vpi_call 2 282 "$finish" {0 0 0};
    %end;
    .thread T_3;
